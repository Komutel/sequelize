    "./_non-semver-use-at-your-own-risk_/*": {
      "types": "./types/*",
      "default": "./lib/*"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "^14.8.0 || >=16.0.0"
  },
  "files": [
    "lib",
    "types",
    "index.js"
  ],
  "license": "MIT",
  "dependencies": {
    "@types/debug": "^4.1.7",
    "@types/inflection": "^1.13.0",
    "@types/validator": "^13.7.5",
    "dayjs": "^1.11.5",
    "debug": "^4.3.4",
    "dottie": "^2.0.2",
    "inflection": "^1.13.2",
    "lodash": "^4.17.21",
    "pg-connection-string": "^2.5.0",
    "retry-as-promised": "^6.0.0",
    "semver": "^7.3.7",
    "sequelize-pool": "^8.0.0",
    "toposort-class": "^1.0.1",
